{
  "name": "ssistudios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "dev:turbo": "bun --bun next dev --turbopack --experimental-turbo",
    "build": "bun --bun next build --experimental-build-mode=compile",
    "build:turbo": "bun --bun next build --turbopack",
    "build:analyze": "ANALYZE=true bun run build",
    "start": "bun --bun next start",
    "start:turbo": "bun --bun next start --turbopack",
    "lint": "bun --bun next lint",
    "lint:fix": "bun --bun next lint --fix",
    "type-check": "bun tsc --noEmit",
    "type-check:watch": "bun tsc --noEmit --watch",
    "clean": "bun rimraf .next out node_modules/.cache",
    "clean:all": "bun run clean && bun rimraf node_modules bun.lock",
    "postinstall": "bun run type-check"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "balanced-match": "^3.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concat-map": "^0.0.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
